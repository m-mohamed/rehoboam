    local commands; commands=()
    _describe -t commands 'rehoboam sprites info commands' commands "$@"
}
(( $+functions[_rehoboam__sprites__list_commands] )) ||
_rehoboam__sprites__list_commands() {
    local commands; commands=()
    _describe -t commands 'rehoboam sprites list commands' commands "$@"
}

if [ "$funcstack[1]" = "_rehoboam" ]; then
    _rehoboam "$@"
else
    compdef _rehoboam rehoboam
fi
